
page {
  position: relative;
  display: flex;
  align-items: center;
  flex-direction: column;
  width: 750rpx;
  background-color: rgb(246, 246, 246);
  padding-bottom: 40rpx;
}

.order-container {
  width: 750rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30rpx;
}
.order-container .coach-info {
  position: relative;
  display: flex;
  flex-direction: column;
  padding: 30rpx;
  width: 630rpx;
  /* height: 300rpx; */
  border-radius: 20rpx;
  background: linear-gradient(to bottom, #f9eade, #FFFFFF);
}
.coach-info .base-info {
  width: 100%;
  height: 180rpx;
  display: flex;
}
.coach-info image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 10rpx;
}

.coach-info .text-info {
  height: 180rpx;
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
}

.coach-info .text-info .name {
  margin-top: 10rpx;
  font-size: 34rpx;
  font-weight: 600;
  color: #1e1d1d;
}
.coach-info .text-info .score {
  margin-left: 20rpx;
  font-size: 36rpx;
  color: #fe9602;
}
.coach-info .text-info .phone {
  margin-left: 20rpx;
  font-size: 30rpx;
  color: #a09f9f;
}
.coach-info .text-info .text {
  margin-left: 10rpx;
  font-size: 26rpx;
  font-weight: 450;
}
.coach-info .text-info .sub {
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  font-weight: 400;
  color: #a09f9f;
}
.coach-info .base-info .prices {
  position: absolute;
  top: 140rpx;
  right: 40rpx;
  display: flex;
  align-items: baseline;
}
.coach-info .base-info .sysbol {
  color: #ef5135;
  font-size: 24rpx;
  line-height: 30rpx;
}
.coach-info .base-info .number {
  color: #ef5135;
  font-size: 36rpx;
  line-height: 30rpx;
}
.coach-info .coach-dec {
  width: 570rpx;
  padding: 30rpx;
  margin-top: 20rpx;
  border-radius: 20rpx;
  background: rgb(246, 246, 246);
  font-size: 26rpx;
  color: rgb(96, 96, 96);
  line-height: 30rpx;
}
.coach-school {
  width: 630rpx;
  height: 40rpx;
  margin-top: 30rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: rgb(96, 96, 96);
  line-height: 30rpx;
}
.coach-school image {
  width: 30rpx;
  height: 30rpx;
}

.selectDate-container {
  position: relative;
  display: flex;
  flex-direction: column;
  padding: 30rpx;
  width: 630rpx;
  min-height: 300rpx;
  border-radius: 20rpx;
  margin-top: 20rpx;
  background: #FFFFFF;
}
.selectDate-container .title {
  font-size: 30rpx;
  color: #1e1d1d;
  font-weight: 500;
}
.selectDate-container .selectDate-options {
  width: 630rpx;
  display: flex;
  flex-wrap: wrap;
}
.selectDate-container .selectDate-options .option {
  position: relative;
  width: 100rpx;
  height: 100rpx;
  margin-top: 30rpx;
  padding: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  border-radius: 10rpx;
  font-size: 28rpx;
  color: #1e1d1d;
  background-color: #eeeeee;
}
.selectDate-container .selectDate-options .option:active {
  background-color: #f9ddb6;
}
.selectDate-container .selectDate-options .active {
  background-color: #f9ddb6;
  border: 2rpx solid #fe9602;
}
.selectDate-container .selectDate-options image {
  position: absolute;
  z-index: 1000;
  bottom: 0;
  right: 0;
  width: 26rpx;
  height: 20rpx;
}
.selectDate-container .timePeriods .active {
  background-color: #f9ddb6;
  border: 2rpx solid #fe9602;
}
.selectDate-container .timePeriods image {
  position: absolute;
  z-index: 1000;
  bottom: 0;
  right: 0;
  width: 26rpx;
  height: 20rpx;
}

.selectDate-container .timePeriods {
  width: 630rpx;
  margin-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
  /* justify-content: space-between; */
  margin-top: 30rpx;
}
.selectDate-container .timePeriods .timePeriod {
  position: relative;
  width: 180rpx;
  height: 110rpx;
  padding: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  border-radius: 10rpx;
  font-size: 28rpx;
  color: #777676;
  margin-bottom: 30rpx;
  background-color: #eeeeee;
}
.selectDate-container .timePeriods .timePeriod .reducible {
  color: #fe9602;
}
.train-container {
  position: relative;
  display: flex;
  align-items: center;
  width: 630rpx;
  min-height: 40rpx;
  padding: 16rpx 0;
}
.train-container .icon {
  font-size: 32rpx;
  color: crimson;
  position: relative;
}
.train-container .label {
  position: relative;
  margin-left: 10rpx;
  font-size: 32rpx;
  width: 150rpx;
  color: rgb(45, 44, 44);
}
.train-container input {
  margin-left: 20rpx;
  height: 40rpx;
  flex: 1;
  font-size: 32rpx;
  color: rgb(45, 44, 44);
}
.train-container .picker {
  margin-left: 40rpx;
  color: #4c4c4c;
}


.other-container {
  position: relative;
  display: flex;
  flex-direction: column;
  padding: 30rpx;
  width: 630rpx;
  border-radius: 20rpx;
  margin-top: 20rpx;
  background: #FFFFFF;
}
.other-list {
  position: relative;
  display: flex;
  flex-direction: column;
  width: 630rpx;
  margin-top: 30rpx;
}
.other-list .other-list-item {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 20rpx 0rpx;
}
.other-list .other-list-item image {
  width: 50rpx;
  height: 50rpx;
}
.other-list .other-list-item .unpaid {
  color: #fe9602;
}
.other-list .other-list-item .paid {
  color: #00c45b;
}
.other-list .other-list-item .cancel {
  color: #aeaeae;
}
.other-list .other-list-item text {
  color: #1e1d1d;
  font-size: 30rpx;
  line-height: 30rpx;
}

.pay-container {
  position: relative;
  display: flex;
  flex-direction: column;
  padding: 30rpx;
  width: 630rpx;
  border-radius: 20rpx;
  margin-top: 20rpx;
  background: #FFFFFF;
}
.pay-list {
  position: relative;
  display: flex;
  flex-direction: column;
  width: 630rpx;
  margin-top: 30rpx;
}
.pay-list .pay-list-item {
  position: relative;
  display: flex;
  align-items: center;
}
.pay-list .pay-list-item image {
  width: 50rpx;
  height: 50rpx;
}
.pay-list .pay-list-item text {
  margin-left: 40rpx;
  color: #1e1d1d;
  font-size: 30rpx;
  line-height: 30rpx;
}
.pay-list .pay-list-item .icon {
  position: absolute;
  right: 0;
  width: 10rpx;
  height: 10rpx;
  border-radius: 100%;
  border: 8rpx solid #5f8be2;
}
.bottom-btn {
  width: 690rpx;
  height: 80rpx;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.bottom-btn .btn {
  margin: 0;
  width: 300rpx;
  height: 80rpx;
  background-color: #fe9602;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #FFF;
  font-size: 30rpx;
  font-weight: 400;
  border-radius: 20rpx;
}
.bottom-btn .cancel {
  background-color: rgb(255, 255, 255);
  color: #4b4a4a;
}

.btn:active {
  background-color: #e1e1e2;
}
/* 隐藏默认的 checkbox */
.checkbox-hidden {
  position: absolute;
  opacity: 0;
  width: 0;
  height: 0;
}