page {
  background-color: #f4f5f7;
}

.swiper-container swiper {
  width: 750rpx;
  height: 412rpx;
}

.swiper-container swiper image {
  width: 100%;
}

.content {
  width: 100%;
  padding-top: 18rpx;
  background-color: #fff;
}

.notice {
  width: 706rpx;
  height: 90rpx;
  background-color: #fff;
  margin: 0 auto;
  position: relative;
  border-radius: 8rpx;
  box-shadow: 0 3rpx 5rpx 1rpx #ccc;
}

.notice-left {
  width: 139rpx;
  height: 42rpx;
  background-color: #fe9602;
  border-radius: 50rpx;
  font-size: 23rpx;
  color: #fff;
  font-family: '微软雅黑';
  text-align: center;
  line-height: 42rpx;
  position: absolute;
  top: 25rpx;
  left: 22rpx;
}

.notice-righ {
  width: 445rpx;
  height: 100%;
  position: absolute;
  right: 66rpx;
  font-size: 24rpx;
  line-height: 88rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.swiper_container {
  height: 100%;
  width: 445rpx;
}

.swiper_item {
  width: 445rpx;
  height: 100%;
  position: absolute;
  font-size: 24rpx;
  line-height: 88rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.icons {
  width: 100%;
  height: 180rpx  !important;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
}

.icons navigator {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.icons navigator image {
  width: 70rpx;
  margin: 20rpx;
}

.footer {
  margin-top: 20rpx;
}

