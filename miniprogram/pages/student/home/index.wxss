page {
  width: 710rpx;
  margin: 0 20rpx;
  /* background-color: #f4f5f7; */
  background-color: #fff;
}

.swiper-container swiper {
  width: 710rpx;
  height: 300rpx;
}

.swiper-container swiper image {
  width: 100%;
}

.content {
  width: 100%;
  padding-top: 18rpx;
  background-color: #fff;
}

.notice {
  width: 710rpx;
  height: 50rpx;
  margin-top: 16rpx;
  position: relative;
  display: flex;
  align-items: center;
  border-radius: 8rpx;
  box-shadow: 0 3rpx 5rpx 1rpx #ccc;
}

.notice-left {
  width: 139rpx;
  height: 42rpx;
  background-color: #fe9602;
  border-radius: 50rpx;
  font-size: 23rpx;
  color: #fff;
  font-family: '微软雅黑';
  text-align: center;
  line-height: 42rpx;
  position: relative;
  left: 22rpx;
}

.notice-righ {
  width: 445rpx;
  height: 100%;
  position: relative;
  margin-left: 66rpx;
  font-size: 24rpx;
  line-height: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.swiper_container {
  height: 100%;
  width: 445rpx;
}

.swiper_item {
  width: 445rpx;
  height: 100%;
  position: absolute;
  font-size: 24rpx;
  line-height: 50rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.icons {
  width: 100%;
  height: 180rpx  !important;
  margin-top: 20rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
}

.icons navigator {
  width: 180rpx;
  /* flex: 1; */
  display: flex;
  flex-direction: column;
  align-items: center;
  box-shadow: 0 3rpx 5rpx 1rpx #ccc;
}

.icons navigator image {
  width: 70rpx;
  margin: 20rpx;
}

.footer {
  margin-top: 20rpx;
}

